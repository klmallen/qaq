# QAQ Game Engine Environment Variables

# Database Configuration
DATABASE_URL="file:./dev.db"

# Authentication Configuration
JWT_SECRET="qaq-game-engine-super-secret-key-2024-development-only-change-in-production"
JWT_EXPIRES_IN="7d"
REFRESH_TOKEN_EXPIRES_IN="30d"

# Development Settings
NODE_ENV="development"
NUXT_DEV_TOOLS="true"

# Project Settings
QAQ_ENGINE_VERSION="1.0.0"
QAQ_DEFAULT_PROJECT_PATH="./projects"

# Logging
LOG_LEVEL="info"
DEBUG_MODE="true"

# Performance Settings
MAX_SCENE_NODES="10000"
MAX_TEXTURE_SIZE="4096"
ENABLE_SHADOWS="true"
ENABLE_PHYSICS="true"

# Editor Settings
AUTO_SAVE_INTERVAL="300000"
BACKUP_COUNT="5"
ENABLE_TELEMETRY="false"
