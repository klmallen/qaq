# QAQ Game Engine Environment Variables

# Database Configuration
# This will be dynamically set based on the current project
DATABASE_URL="file:./dev.db"

# Development Settings
NODE_ENV="development"
NUXT_DEV_TOOLS="true"

# Project Settings
QAQ_ENGINE_VERSION="1.0.0"
QAQ_DEFAULT_PROJECT_PATH="./projects"

# Logging
LOG_LEVEL="info"
DEBUG_MODE="true"

# Performance Settings
MAX_SCENE_NODES="10000"
MAX_TEXTURE_SIZE="4096"
ENABLE_SHADOWS="true"
ENABLE_PHYSICS="true"

# Editor Settings
AUTO_SAVE_INTERVAL="300000"  # 5 minutes in milliseconds
BACKUP_COUNT="5"
ENABLE_TELEMETRY="false"
